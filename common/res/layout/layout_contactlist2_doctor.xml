<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_contactlist2"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#fdeeda" >

    <RelativeLayout
        android:id="@+id/contact2_title"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:visibility="gone" >

        <TextView
            android:id="@+id/PhoneNumber"
            style="@style/DialNum"
            android:layout_width="fill_parent"
            android:layout_height="40dp"
            android:visibility="gone" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:scaleType="fitXY"
            android:src="#00ffffff" />

        <ImageButton
            android:id="@+id/notification_none"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="7dp"
            android:background="@null"
            android:src="@drawable/btn_notification_none" />

        <ImageButton
            android:id="@+id/notification_unread"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="7dp"
            android:background="@null"
            android:src="@drawable/btn_notification_unread"
            android:visibility="gone" />

        <ImageView
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/all_light_icn" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/wifiphone_tab_btn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal" 
        android:background="@drawable/img_call_tab_bottom">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1" >

            <Button
                android:id="@+id/bottom_contacts"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@drawable/btn_call_contacts_highlight" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:paddingTop="30dp"
                android:text="@string/contacts"
                style="@style/ContactsBottomTabHighLight" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_weight="1" >

            <Button
                android:id="@+id/bottom_keypad"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@drawable/selector_tab_keypad" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:paddingTop="30dp"
                android:text="@string/keypad"
                style="@style/ContactsBottomTab" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_weight="1" >

            <Button
                android:id="@+id/bottom_history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@drawable/selector_tab_history" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:paddingTop="30dp"
                android:text="@string/call_history"
                style="@style/ContactsBottomTab" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/contact2_all"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@id/wifiphone_tab_btn"
        android:layout_below="@id/contact2_title"
        android:layout_gravity="center"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/contacts"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/img_call_tab_top"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/userinfo_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_btn_vc_info" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" 
                android:layout_centerInParent="true">

                <TextView
                    style="@style/TabTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/contacts" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" 
                android:layout_alignParentRight="true">
            <Button 
                android:id="@+id/all_expandlist_ctl_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/collapse"
                android:textColor="#723e1a"
                android:paddingRight="10dp"
                android:background="@drawable/selector_btn_expandlist_ctl"/>
			</RelativeLayout>
            <LinearLayout
                android:id="@+id/ll_contacts"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:orientation="horizontal" 
                android:visibility="gone">

                <Button
                    android:id="@+id/call_contacts"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginRight="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/btn_contact_highlight"
                    android:clickable="false"
                    android:gravity="center"
                    android:paddingLeft="30dp"
                    android:text="@string/contacts"
                    android:textColor="#168995"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_call_history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone" >

                <Button
                    android:id="@+id/call_history"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/btn_history"
                    android:gravity="center"
                    android:paddingLeft="20dp"
                    android:text="@string/call_history"
                    android:textColor="#FFFFFF"
                    android:textSize="18sp" />
            </LinearLayout>
        </RelativeLayout>
        <LinearLayout 
            android:id="@+id/mBroadcastList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        </LinearLayout>
        
        <ListView
            android:id="@android:id/list"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:cacheColorHint="#00000000"
            android:divider="@drawable/contact_separateline"
            android:listSelector="#00000000"
            android:padding="5dip"
            android:visibility="gone" />

        <ExpandableListView
            android:id="@+id/mExpandableListView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:cacheColorHint="#00000000"
            android:divider="@null"
            android:groupIndicator="@drawable/group_indicator_ori"
            android:listSelector="#00000000" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tab_btn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:visibility="gone" >

        <ImageButton
            android:id="@+id/CarePlan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="1dp"
            android:layout_weight="1"
            android:background="@null"
            android:src="@drawable/btn_measure" />

        <ImageButton
            android:id="@+id/Profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="1dp"
            android:layout_weight="1"
            android:background="@null"
            android:src="@drawable/btn_profile" />

        <ImageButton
            android:id="@+id/Canlendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="1dp"
            android:layout_weight="1"
            android:background="@null"
            android:src="@drawable/btn_calendar" />

        <ImageButton
            android:id="@+id/Phonebook"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:layout_weight="1"
            android:background="@null"
            android:clickable="false"
            android:src="@drawable/btn_contact"
            android:textSize="6pt" />
    </LinearLayout>

    <TextView
        android:id="@+id/versionName"
        android:layout_width="fill_parent"
        android:layout_height="25dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:paddingBottom="3dp"
        android:text=""
        android:textColor="#0C746E"
        android:textSize="12sp"
        android:visibility="gone" />

    

</RelativeLayout>